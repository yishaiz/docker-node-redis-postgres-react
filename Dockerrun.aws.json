{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "yishaiz/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "yishaiz/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "yishaiz/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "yishaiz/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
          "hostPort": 80
          "ContainerPort": 80,
      ]
    }
  ]
}
